    <td><%= jardin.edad_entrada %></td>
    <td><%= jardin.edad_salida %></td>
    <td><%= jardin.horario %></td>
    <td><%= jardin.vacantes %></td>
    <td><%= jardin.latitud %></td>
    <td><%= jardin.longitud %></td>
    <td><%= link_to 'Show', jardin %></td>
    <td><%= link_to 'Edit', edit_jardin_path(jardin) %></td>
    <td><%= link_to 'Destroy', jardin, :confirm => 'Are you sure?', :method => :delete %></td>
