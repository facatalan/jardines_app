<% @jardins.each do |jardin| %>
  <tr>
    <td><%= jardin.nombre %></td>
    <td><%= jardin.direccion %></td>
    <td><%= jardin.telefono %></td>
    <td><%= jardin.web %></td>
    <td><%= jardin.email %></td>
    <td><%= jardin.capacidad %></td>
    <td><%= jardin.camaras %></td>
    <td><%= jardin.ninos_x_tia %></td>
    <td><%= jardin.matricula %></td>
